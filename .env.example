# Gemini API Key from Google AI Studio (https://aistudio.google.com/)
GEMINI_API_KEY=

# Google OAuth Credentials (https://console.cloud.google.com/)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Production URL (e.g., https://merchmagic.up.railway.app)
# This must NOT have a trailing slash.
APP_URL=

# Session Secret (Any long random string for cookie encryption)
SESSION_SECRET=

# Stripe Credentials (Optional - for Pro upgrades)
STRIPE_SECRET_KEY=
VITE_STRIPE_PUBLISHABLE_KEY=

# Android App Details (Required for Bubblewrap/TWA to hide URL bar)
# Get SHA256 from your Bubblewrap build output
ANDROID_PACKAGE_NAME=com.merchmagic.app
ANDROID_SHA256=
